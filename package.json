{
  "name": "vap",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ty-richard/vap"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "release": "yarn run standard-version --no-verify",
    "release:pre": "yarn run release --prerelease rc",
    "release:major": "yarn run release --release-as major",
    "release:minor": "yarn run release --release-as minor",
    "release:patch": "yarn run release --release-as patch",
    "release:alpha": "yarn run release --release-as patch --prerelease alpha",
    "release:alpha:minor": "yarn run release --release-as minor --prerelease alpha",
    "release:alpha:major": "yarn run release --release-as major --prerelease alpha"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/forms": "^0.5.10",
    "@vercel/speed-insights": "^1.2.0",
    "clsx": "^2.1.1",
    "commitlint": "^19.7.1",
    "next": "15.1.7",
    "pocketbase": "^0.25.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "standard-version": "^9.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
